{
    "name": "@via-profit/dataloader",
    "support": "https://via-profit.ru",
    "version": "1.0.2",
    "description": "Via Profit Redis-Dataloader",
    "main": "dist/index.js",
    "engines": {
        "node": ">= 14.0.0",
        "npm": ">= 6.14.8"
    },
    "keywords": [
        "graphql",
        "via-profit",
        "dataloader"
    ],
    "scripts": {
        "dist": "yarn lint && yarn test && yarn build:dist",
        "analyze": "cross-env ANALYZE=true yarn build:dist",
        "debug": "yarn build:dev && yarn node --inspect-brk=9229 ./build/index.js",
        "build:dev": "cross-env NODE_ENV=development webpack --config ./webpack/webpack-config-dev.ts",
        "build:dist": "webpack --config ./webpack/webpack-config-prod.ts",
        "start": "yarn build:dev --watch",
        "test": "jest",
        "lint": "eslint --ext .ts ./src/ --fix .",
        "postversion": "git push --tags",
        "release:patch": "yarn version --patch",
        "release:minor": "yarn version --minor",
        "release:major": "yarn version --major",
        "release:prerelease": "yarn version --prerelease"
    },
    "repository": {
        "type": "git",
        "url": "git@github.com:via-profit/dataloader.git"
    },
    "author": {
        "name": "Via Profit",
        "email": "promo@via-profit.ru",
        "url": "https://dev.via-profit.ru"
    },
    "contributors": [
        "Vasily Novosad <delhsmail@gmail.com>",
        "Pavel Natalin <trubonru@gmail.com>"
    ],
    "license": "MIT",
    "peerDependencies": {
        "ioredis": "^4.27.2"
    },
    "devDependencies": {
        "@jagi/jest-transform-graphql": "^1.0.2",
        "@types/bluebird": "^3.5.29",
        "@types/cors": "^2.8.6",
        "@types/ioredis": "^4.26.4",
        "@types/jest": "^24.0.23",
        "@types/node": "^14.14.16",
        "@types/nodemon": "^1.19.0",
        "@types/supertest": "^2.0.8",
        "@types/validator": "^12.0.1",
        "@types/webpack": "^4.41.24",
        "@types/webpack-bundle-analyzer": "^3.9.0",
        "@types/webpack-merge": "^4.1.5",
        "@types/webpack-node-externals": "^1.7.0",
        "@typescript-eslint/eslint-plugin": "^4.6.1",
        "@typescript-eslint/parser": "^4.6.1",
        "@via-profit-services/core": "^1.1.9",
        "@via-profit-services/eslint-config": "^0.1.11",
        "@zerollup/ts-transform-paths": "^1.7.11",
        "cors": "^2.8.5",
        "cross-env": "^6.0.3",
        "dotenv": "^8.2.0",
        "eslint": "^7.12.1",
        "eslint-config-prettier": "^6.15.0",
        "eslint-config-standard": "^16.0.1",
        "eslint-import-resolver-node": "^0.3.4",
        "eslint-import-resolver-typescript": "^2.3.0",
        "eslint-import-resolver-webpack": "^0.13.0",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-prettier": "^3.1.4",
        "eslint-plugin-promise": "^4.2.1",
        "eslint-plugin-react": "^7.21.5",
        "eslint-plugin-standard": "^4.0.2",
        "graphql-tag": "^2.11.0",
        "html-loader": "^1.3.2",
        "husky": "^4.2.3",
        "jest": "^24.9.0",
        "jest-transform-graphql": "^2.1.0",
        "nodemon": "^2.0.6",
        "nodemon-webpack-plugin": "^4.3.2",
        "null-loader": "^4.0.0",
